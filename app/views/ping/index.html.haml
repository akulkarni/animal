%div{"ng-controller" => "DateCtrl"}
  .ping-date
    %i.icon-backward{"ng-click" => "prevDate()"}
    %span.today {{today}}
    %i.icon-forward{"ng-click" => "nextDate()"}

  .ping-time-row{"ng-repeat" => "time in times", 
                 "ng-click" => "toggleTime(time)",
                 "ng-class" => "{selected: getSelectedState(time)}"}
    {{time}}





